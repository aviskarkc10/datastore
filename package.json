{
  "name": "@verida/datastore",
  "version": "0.4.0",
  "description": "Verida Datastore",
  "main": "dist/app.js",
  "author": "Verida",
  "license": "Apache-2.0",
  "scripts": {
    "test": "mocha -mocha --require @babel/polyfill --require @babel/register './test/**/*.js'",
    "generate-api-docs": "./node_modules/.bin/jsdoc ./ -r -c .jsdoc.conf -d apidocs -u tutorials --readme README.api.md",
    "serve-docs": "~/.npm-global/bin/docsify serve docs",
    "build": "./node_modules/.bin/babel src -d dist",
    "preinstall": "npm install --ignore-scripts",
    "prepublish": "npm run build",
    "postinstall": "npm run build"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "dependencies": {
    "@verida/did-helper": "^0.4.3",
    "crypto-pouch": "git+https://github.com/tahpot/crypto-pouch.git",
    "did-document": "github:arablocks/did-document",
    "did-jwt": "^4.0.0",
    "did-jwt-vc": "^0.1.4",
    "did-resolver": "^1.1.0",
    "ethers": "^4.0.47",
    "fs": "0.0.1-security",
    "json-schema-ref-parser": "^7.1.2",
    "json-schema-resolve-allof": "^1.5.0",
    "lodash": "^4.17.15",
    "ono": "^7.1.2",
    "pouchdb": "^7.2.1",
    "pouchdb-find": "^7.1.1",
    "store": "^2.0.12",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.0",
    "url-exists": "^1.0.3",
    "uuid": "^3.3.3",
    "web3": "^1.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "jsdoc": "^3.6.3",
    "minami": "^1.2.3",
    "mocha": "^7.0.0"
  }
}
